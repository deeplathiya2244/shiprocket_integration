<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="shiprocket_channels_id_tree_view" model="ir.ui.view">
            <field name="name">lax.shiprocket.channel.tree.view</field>
            <field name="model">lax.shiprocket.channel</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="shiprocket_channel_id"/>
                    <field name="name"/>
                    <field name="status"/>
                    <field name="gmail"/>
                </tree>
            </field>
        </record>

        <record id="vehicle_payment_option_form_view" model="ir.ui.view">
            <field name="name">lax.shiprocket.channel.form.view</field>
            <field name="model">lax.shiprocket.channel</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="shiprocket_channel_id"/>
                            <field name="name"/>
                            <field name="status"/>
                            <field name="gmail"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="action_channel" model="ir.actions.act_window">
            <field name="name">Channel</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">lax.shiprocket.channel</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>